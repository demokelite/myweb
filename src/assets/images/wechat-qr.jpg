<svg xmlns="http://www.w3.org/2000/svg" width="200" height="200" viewBox="0 0 200 200">
  <rect width="200" height="200" fill="white" stroke="#ddd" stroke-width="2"/>
  <!-- 简单的二维码图案 -->
  <g fill="black">
    <!-- 左上角方块 -->
    <rect x="20" y="20" width="60" height="60"/>
    <rect x="30" y="30" width="40" height="40" fill="white"/>
    <rect x="40" y="40" width="20" height="20"/>
    
    <!-- 右上角方块 -->
    <rect x="120" y="20" width="60" height="60"/>
    <rect x="130" y="30" width="40" height="40" fill="white"/>
    <rect x="140" y="40" width="20" height="20"/>
    
    <!-- 左下角方块 -->
    <rect x="20" y="120" width="60" height="60"/>
    <rect x="30" y="130" width="40" height="40" fill="white"/>
    <rect x="40" y="140" width="20" height="20"/>
    
    <!-- 中心区域 -->
    <rect x="90" y="90" width="20" height="20"/>
    <rect x="70" y="70" width="10" height="10"/>
    <rect x="120" y="70" width="10" height="10"/>
    <rect x="70" y="120" width="10" height="10"/>
    <rect x="120" y="120" width="10" height="10"/>
    
    <!-- 随机点 -->
    <rect x="40" y="80" width="10" height="10"/>
    <rect x="60" y="100" width="10" height="10"/>
    <rect x="80" y="140" width="10" height="10"/>
    <rect x="100" y="60" width="10" height="10"/>
    <rect x="120" y="100" width="10" height="10"/>
    <rect x="140" y="80" width="10" height="10"/>
    <rect x="160" y="140" width="10" height="10"/>
  </g>
  <text x="100" y="185" font-family="Arial" font-size="12" text-anchor="middle" fill="#666">微信支付</text>
</svg>